---
import Layout from "../layouts/Layout.astro";
import Navigation from "../components/Navigation.astro"
import Footer from "../components/Footer.astro"
import CaseTitle from "../components/CaseTitle.astro"
import Frames from "../components/Frames.astro"
import BlenderGrid from "../components/BlenderGrid.astro";


---
<Layout>
    <BlenderGrid></BlenderGrid>
    <CaseTitle>blender</CaseTitle>
    <style>
        body {
            background-image: url("/images/blender_b.jpg");
            background-repeat: no-repeat;
            background-size: cover;
            background-position: 100%;
            backdrop-filter: blur(100px);
        }
        </style>
    <Footer next="/axiDraw" previous="/work"></Footer>
    <Frames></Frames>
</Layout>