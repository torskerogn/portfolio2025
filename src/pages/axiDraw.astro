---
import Layout from "../layouts/Layout.astro"
import Footer from "../components/Footer.astro"
import CaseTitle from "../components/CaseTitle.astro"
import Frames from "../components/Frames.astro"
import AxiDrawGrid from "../components/AxiDrawGrid.astro"
---

<Layout>
	<AxiDrawGrid></AxiDrawGrid>
	<CaseTitle>axiDraw</CaseTitle>
    <style>
            body {
		background-image: url("/images/c_b.jpg");
		background-repeat: no-repeat;
		background-size: cover;
		background-position: 100%;
		backdrop-filter: blur(100px);
	}
    </style>
    <Footer next="/processing" previous="/blender"></Footer>
    <Frames></Frames>
</Layout>